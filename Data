CREATE SEQUENCE dd; 
CREATE TABLE ts ( 
  CHART_NUMBER CHAR(20) NOT NULL DEFAULT to_char(nextval('dd'), 'EMP0000FM'), 
    FIRST_NAME    VARCHAR(20)   NOT NULL,
    LAST_NAME     VARCHAR (20)  NOT NULL,
    MIDDLE_NAME   VARCHAR(20),
  	SEX VARCHAR(20),
  	RACE VARCHAR(20),
    DOB DATE,
    PRIMARY KEY(CHART_NUMBER)

);
INSERT INTO ts(FIRST_NAME,LAST_NAME,MIDDLE_NAME,SEX) VALUES('NIKITA','MIRCHANDANI','M','FEMALE');
select * from ts;
 
CREATE TABLE ADDRESS(
  ADDRESS_ID SERIAL PRIMARY KEY,
  ADDRESS_TYPE VARCHAR(20) NOT NULL,
  STREET VARCHAR(20) ,
  CITY VARCHAR(20),
  ZIP INT,
  STATE VARCHAR(20),
  COUNTRY VARCHAR(20),
  CHART_NUMBER VARCHAR(20),
  CONSTRAINT user_id FOREIGN KEY(CHART_NUMBER)
        REFERENCES ts(CHART_NUMBER)
  );
  
  INSERT INTO ADDRESS(ADDRESS_TYPE,STREET,CITY,ZIP,CHART_NUMBER) VALUES('HOME','STREET','NADIAD',387630,'EMP0001');

